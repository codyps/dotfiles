[push]
	default = matching
[color]
	ui = auto
[alias]
	post = !sh -c 'git format-patch --stdout $1 | ix' -
	ci = commit -v
	st = status
	co = checkout
	b = branch -v
	dc = describe --contains
	fp = format-patch -k -M -N
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	sm = submodule
	submod = submodule
